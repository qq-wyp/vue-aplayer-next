{
  "name": "vue-aplayer-next",
  "version": "2.0.0",
  "description": "Easy-to-use music player for Vue 3.x",
  "main": "dist/vue-aplayer.min.js",
  "files": [
    "src",
    "!src/demo/*",
    "dist/*.js"
  ],
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack serve --config webpack.demo.config.js",
    "build": "cross-env NODE_ENV=production && webpack --progress",
    "build:demo": "cross-env NODE_ENV=production && webpack --progress --config webpack.demo.config.js",
    "prepublishOnly": "yarn run build",
    "predeploy": "pnpm add hls.js && yarn run build:demo && cross-env 'vue-aplayer.js.org' > demo/CNAME",
    "test": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SevenOutman/vue-aplayer.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "aplayer",
    "vue-aplayer",
    "music-player",
    "html5",
    "component",
    "vue-compoents"
  ],
  "author": {
    "name": "Doma",
    "updater": "xiangyang",
    "email": "leishenghao@126.com",
    "url": "https://github.com/SevenOutman"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SevenOutman/vue-aplayer/issues"
  },
  "homepage": "https://github.com/SevenOutman/vue-aplayer#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-stage-2": "^7.8.3",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "autoprefixer": "9.6.1",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "2.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "file-loader": "3.0.1",
    "hls.js": "0.12.4",
    "html-webpack-plugin": "3.2.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.6.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "0.23.1",
    "svg-inline-loader": "0.8.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "1.1.2",
    "vue-html-loader": "1.2.4",
    "vue-loader": "^17.0.0",
    "webpack": "4.39.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vue": "^3.2.37"
  }
}
